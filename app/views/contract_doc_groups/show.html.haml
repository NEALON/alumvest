= render 'investments/navigation', :active => 'investment_documents', :campaign => @campaign

%table.table
  %thead
    %tr
      %td
        Document name
      %td
        Document status
  - @signings.each do |signing|
    %tbody
      %tr
        %td
          = link_to_unless signing.status == 'completed', signing.document.name, signing.envelope.get_recipient_view('investor')
        %td
          = signing.status_for_end_user

-# %iframe.col-md-12{:src => @signing_url, :style => 'min-height: 500px'}