%h3 Our Team

- if cpf = @company_worksheet.company_personnel_form

  .row{:style => "margin-bottom: 1em"}
    .large-12.columns
      - if founders = cpf.founders
        %h4 Founders
        - cpf.founders.each do |founder|
          = founder.full_name

  .row{:style => "margin-bottom: 1em"}
    .large-12.columns
      - if team_members = cpf.team_members
        %h4 Team Members
        - cpf.team_members.each do |m|
          = m.full_name

  .row{:style => "margin-bottom: 1em"}
    .large-12.columns
      - if board_members = cpf.board_members
        %h4 Board Members
        - cpf.board_members.each do |m|
          = m.full_name

  .row{:style => "margin-bottom: 1em"}
    .large-12.columns
      - if advisors = cpf.advisors
        %h4 Advisors
        - cpf.advisors.each do |m|
          = m.full_name

  .row{:style => "margin-bottom: 1em"}
    .large-12.columns
      - if legal_counsel = cpf.legal_counsel
        %h4 Legal Counsel
        = cpf.legal_counsel.first.full_name