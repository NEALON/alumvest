- content_for :wrapper_classes, 'no-pad-top'

.masthead-campaign
  .container
    .media
      .pull-left.logotype-campaign
        = image_tag('campaign/logotype.jpg', :width => 149)
      .media-body
        %h2.upper.title-campaign.text-lato.text-normal Rocking App
        %h3.summary-campaign.text-lato.text-book Connecting Doctors and Health App
        %h4.school-campaign.text-lato.text-book Universties : New york University, Columbia University, Harvard University
        %ul.bottom-info.list-unstyled.text-lato.text-book
          %li 
            = "Lead Investors : "
            = link_to 'Lynn Chen', '#'
            = ","
            = link_to 'Sasha', '#'
            = "," 
            = link_to 'Alan Hua', '#'
            = "."
          %li Locations : New youk, Columbia, Canada
/ END .masthead-campaign

.navbar-campaign
  %nav.navbar
    .container
      %ul.nav.navbar-nav.text-open-sans.navbar-left.strong 
        %li.active
          = link_to '#' do
            %span.top-icon
              %i.fa.fa-th
            %span Company
        %li
          = link_to '#' do
            %span.top-icon
              %i.fa.fa-users
            %span Team
        %li
          = link_to '#' do
            %span.top-icon
              %i.fa.fa-file-text-o
            %span Investment Terms
        %li
          = link_to '#' do
            %span.top-icon
              %i.fa.fa-question
            %span Faqâ€™s
        %li
          = link_to '#' do
            %span.top-icon
              %i.fa.fa-repeat
            %span Updates
        %li
          = link_to '#' do
            %span.top-icon
              %i.fa.fa-user
              %i.fa.fa-plus
            %span Join Us
      %ul.nav.navbar-nav.navbar-right.text-open-sans.strong
        %li
          %p.navbar-text
            %i.fa.fa-share-alt
            Share it on :
        %li
          = link_to '#', :data => {:toggle => "tooltip", :placement => "top", :container => 'body'}, :title => "Share on Facebook" do
            %i.fa.fa-facebook-square
        %li
          = link_to '#', :data => {:toggle => "tooltip", :placement => "top", :container => 'body'}, :title => "Share on Twitter" do
            %i.fa.fa-twitter-square
        %li
          = link_to '#', :data => {:toggle => "tooltip", :placement => "top", :container => 'body'}, :title => "Share on LinkedIn" do
            %i.fa.fa-linkedin-square
                
/ END .navbar-campaign

